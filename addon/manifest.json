{
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "automatic_dictionary_extension@jordi_beltran.net",
      "strict_min_version": "102.0",
      "strict_max_version": "117.0"
    }
  },
  "name": "Automatic Dictionary",
  "description": "Extension that remembers the language of the email when composing.",
  "version": "3.2.4",
  "default_locale": "en",

  "options_ui": {
    "page": "options.html"
  },

  "background": {
    "page": "background.html"
  },

  "permissions": [ "compose", "storage", "notifications" ]
}
